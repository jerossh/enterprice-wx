<view class="container">
    <view class="brand-list" >
        <navigator url="../brandDetail/brandDetail?id={{''}}"  class="item" wx:for="{{brandList}}" >
            <view class="img-bg">
                <image src="{{'https://kapuw.net' + item.img}}" background-size="cover"></image>
            </view>
            <view class="txt-box">
                <view class="line">
                    <text class="name" >{{item.name}}</text>
                    <text class="s">|</text>
                    <text class="price">{{item.floor_price}}元起</text>
                </view>
            </view>
        </navigator >
    </view>
</view>